{
  "headers": [
    {
      "source": "/sw.js",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/:match*", "destination": "https://example.com/:match*" },
    { "source": "/", "destination": "https://example.com/" }
  ]
}
